<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="org.portlets.lia.mine.model.IRIssue" />
	<class name="org.portlets.lia.mine.model.impl.IRIssueImpl" table="IR_IRIssue">
		<cache usage="read-write" />
		<id name="issueId" type="long">
			<generator class="assigned" />
		</id>
		<property name="summary" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="requester" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="priority" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>